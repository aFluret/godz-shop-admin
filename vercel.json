{
  "version": 2,
  "builds": [
    {
      "src": "admin.html",
      "use": "@vercel/static"
    },
    {
      "src": "admin.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/admin.html"
    }
  ],
  "headers": {
    "/*": [
      {
        "key": "Content-Security-Policy",
        "value": "default-src 'self'; script-src 'self' 'unsafe-eval';"
      }
    ]
  }
}
